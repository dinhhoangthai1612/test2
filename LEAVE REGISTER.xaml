LEFT JOIN AWADLIB.EAP#09 NL ON E9.MY_FORM_ID=NL.MY_FORM_ID AND NL.ELEMENT_ID IN ('combo_dhxId_Eq1slw','combo_dhxId_bUFlPa','combo_dhxId_4EC6m9')
LEFT JOIN AWADLIB.EAP#09 MT ON E9.MY_FORM_ID=MT.MY_FORM_ID AND MT.ELEMENT_ID IN ('combo_dhxId_fyshPQ','combo_dhxId_05iaJO','combo_dhxId_KbcrpN')

WHERE E9.ELEMENT_ID IN ('input_dhxId_nvuUeK') AND MT.ELEMENT_VALUE&lt;&gt;''
AND E9.MY_FORM_ID IN 
