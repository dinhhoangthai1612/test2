        <Assign sap:VirtualizedContainerService.HintSize="334,64" sap2010:WorkflowViewState.IdRef="Assign_351">
          <Assign.To>
            <OutArgument x:TypeArguments="x:String">[strSQL]</OutArgument>
          </Assign.To>
          <Assign.Value>
            <InArgument x:TypeArguments="x:String" xml:space="preserve">SELECT EAP_NO,FORM_TITLE,SEND_RESULT,TRIM(ERROR_MSG) ERROR_MSG,CREATE_DATE,TYPE

FROM (
SELECT P8.PO_REQ_NO AS EAP_NO,E3.FORM_TITLE,
SEND_RESULT,ERROR_MSG,VARCHAR_FORMAT(P8.CREATE_DATE,'YYYYMMDD HH24MISS') CREATE_DATE, 'PO_NT' TYPE
,IPADDRESS,STARTDAY,ENDDAY,STATUS,REVEIVEDTIME,USERNAME
FROM AWADLIB.PO#P08 P8
INNER JOIN AWADLIB.PO#P07 P7 ON P8.PO_REQ_NO=P7.PO_NO
INNER JOIN AWADLIB.PO#P09 P9 ON P7.PO_REQ_NO=P9.PO_REQ_NO
INNER JOIN AWADLIB.EAP#03 E3 ON P9.FORM_ID=E3.FORM_ID
LEFT JOIN RPALIB.ROBOT_LOG_NT ON PROCESSNAME='ISSUE PO SYSTEM' AND P8.PO_REQ_NO = SUBSTRING(FILENAME,INSTR(FILENAME, '_RPA(Issue_PO)_')+15,13) AND LOWER(FILENAME) LIKE 'ykk_vis_info@ykk.com.vn%'
WHERE P8.CREATE_DATE BETWEEN '((1))' AND '((2))' AND UPPER(P8.RECIPIENT) LIKE 'ROBOTICSNT%'



UNION

SELECT MY_FORM_ID AS EAP_NO,E3.FORM_TITLE,SEND_RESULT,ERROR_MSG,VARCHAR_FORMAT(E12.CREATE_DATE,'YYYYMMDD HH24MISS') CREATE_DATE,'EAP_NT' TYPE,
IPADDRESS,STARTDAY,ENDDAY,STATUS,REVEIVEDTIME,USERNAME
FROM AWADLIB.EAP#12 E12
INNER JOIN AWADLIB.EAP#03 E3 ON E12.FORM_ID=E3.FORM_ID
LEFT JOIN RPALIB.ROBOT_LOG_NT ON PROCESSNAME='EXTRACT EAP' AND MY_FORM_ID = SUBSTRING(FILENAME,INSTR(FILENAME, 'Extract EAP Form_')+17,10) AND LOWER(FILENAME) LIKE 'ykk_vis_info@ykk.com.vn%'
WHERE E12.CREATE_DATE BETWEEN '((1))' AND '((2))' AND UPPER(E12.RECIPIENT) LIKE 'ROBOTICSNT%'

--HANAM ykkhanoi_vis_info
UNION 
SELECT EAP_NO,FORM_TITLE,SEND_RESULT,ERROR_MSG,VARCHAR_FORMAT(TO_DATE(CREATE_DATE, 'MM/DD/YYYY HH24:MI:SS'),'YYYYMMDD HH24MISS') CREATE_DATE,TYPE,
IPADDRESS,STARTDAY,ENDDAY,STATUS,REVEIVEDTIME,USERNAME
FROM RPALIB.YKKVIS_HN
LEFT JOIN RPALIB.ROBOT_LOG_NT LOG_EAP ON LOG_EAP.PROCESSNAME='EXTRACT EAP' AND EAP_NO = SUBSTRING(LOG_EAP.FILENAME,INSTR(LOG_EAP.FILENAME, 'Extract EAP Form_')+17,10) AND LOWER(FILENAME) LIKE 'ykkhanoi_vis_info@ykk.com.vn%'
--LEFT JOIN RPALIB.ROBOT_LOG_NT LOG_PO ON LOG_PO.PROCESSNAME='ISSUE PO SYSTEM' AND EAP_NO = SUBSTRING(LOG_PO.FILENAME,INSTR(LOG_PO.FILENAME, '_RPA(Issue_PO)_')+15,13)
WHERE TO_DATE(CREATE_DATE, 'MM/DD/YYYY HH24:MI:SS') BETWEEN '((1))' AND '((2))' AND TYPE='EAP_HN'

UNION

SELECT EAP_NO,FORM_TITLE,SEND_RESULT,ERROR_MSG,VARCHAR_FORMAT(TO_DATE(CREATE_DATE, 'MM/DD/YYYY HH24:MI:SS'),'YYYYMMDD HH24MISS') CREATE_DATE,TYPE,
IPADDRESS,STARTDAY,ENDDAY,STATUS,REVEIVEDTIME,USERNAME
FROM RPALIB.YKKVIS_HN
--LEFT JOIN RPALIB.ROBOT_LOG_NT LOG_EAP ON LOG_EAP.PROCESSNAME='EXTRACT EAP' AND EAP_NO = SUBSTRING(LOG_EAP.FILENAME,INSTR(LOG_EAP.FILENAME, 'Extract EAP Form_')+17,10)
LEFT JOIN RPALIB.ROBOT_LOG_NT LOG_PO ON LOG_PO.PROCESSNAME='ISSUE PO SYSTEM HN' AND EAP_NO = SUBSTRING(LOG_PO.FILENAME,INSTR(LOG_PO.FILENAME, '_RPA(Issue_PO)_')+15,13) AND LOWER(FILENAME) LIKE 'ykkhanoi_vis_info@ykk.com.vn%'
WHERE TO_DATE(CREATE_DATE, 'MM/DD/YYYY HH24:MI:SS') BETWEEN '((1))' AND '((2))' AND TYPE='PO_HN'
) A
LEFT JOIN RPALIB.SERVER_NT ON ROBOTNAME IN ('EXTRACT EAP','ISSUE PO SYSTEM','ISSUE PO SYSTEM HN') AND FILENAME LIKE '%'||EAP_NO||'%'
WHERE FORM_TITLE LIKE '((3))%' AND TYPE='EAP_NT'
GROUP BY EAP_NO,FORM_TITLE,SEND_RESULT,ERROR_MSG,CREATE_DATE,TYPE
ORDER BY TO_DATE(CREATE_DATE, 'YYYYMMDD HH24MISS') </InArgument>
