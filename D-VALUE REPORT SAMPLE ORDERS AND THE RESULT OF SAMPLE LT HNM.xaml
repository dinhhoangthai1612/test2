                                <ui:AssignOperation sap2010:WorkflowViewState.IdRef="AssignOperation_393">
                                  <ui:AssignOperation.To>
                                    <OutArgument x:TypeArguments="x:String">[query_Book1]</OutArgument>
                                  </ui:AssignOperation.To>
                                  <ui:AssignOperation.Value>
                                    <InArgument x:TypeArguments="x:String" xml:space="preserve">SELECT ORDN5C AS Order, OINU5C AS InputDate, OCNU5C ConfirmDate, RDLU5C RequestDate,
(SELECT MIN(OALU5B) FROM WAVEDLIB.S5B00 WHERE ORDN5B=ORDN5C GROUP BY ORDN5B) Arr_date,
(SELECT MAX(PCPU9F) FROM WAVEDLIB.F9F00 WHERE RLTN9F=ORDN5C GROUP BY RLTN9F) LASTCOMP,
(SELECT MAX(PAKU6P) FROM WAVEDLIB.S6P00 WHERE ORDN6P=ORDN5C GROUP BY ORDN6P) LASTPACK,
MAX(NDCU5F) AS EST_DELI, Delivery, SMPF5C AS SampleFlag, Sum(AAFQ5F) AS FINISHQTY,Sum(PAKQ5F) AS PACKEDQTY, Sum(DCNQ5F) AS DELIVERYQTY,
(SELECT SUM(ORRQ5E) FROM WAVEDLIB.S5E00 WHERE ORDN5E=ORDN5C GROUP BY ORDN5E) SUM_QTY, NCMF5C 

FROM WAVEDLIB.S5C00
INNER JOIN WAVEDLIB.S5F00 S5F ON ORDN5C = S5F.ORDN5F  
INNER JOIN  (SELECT ORDN5F,MAX(DLVU5F) Delivery 
			FROM WAVEDLIB.S5F00 
			INNER JOIN WAVEDLIB.S5C00 ON ORDN5C=ORDN5F 
			GROUP BY ORDN5F,SMPF5C,PLTC5C 
			HAVING MAX(DLVU5F)=( SELECT MAX(C2.YMDU02) FROM WAVEDLIB.C0200 C1 
			INNER JOIN WAVEDLIB.C0200 C2 ON C2.WDSV02 = C1.WDSV02  AND C2.DPTC02=C1.DPTC02 AND C2.HTYC02=0 
			WHERE C1.DPTC02='01' AND C1.YMDU02=?) 
			AND SMPF5C='1' AND PLTC5C='2') A ON ORDN5C=A.ORDN5F 


GROUP BY ORDN5C, OINU5C, OCNU5C, RDLU5C, SMPF5C, NCMF5C,Delivery</InArgument>
                                  </ui:AssignOperation.Value>
                                </ui:AssignOperation>
                                <ui:AssignOperation sap2010:WorkflowViewState.IdRef="AssignOperation_394">
                                  <ui:AssignOperation.To>
                                    <OutArgument x:TypeArguments="x:String">[query_Book2]</OutArgument>
                                  </ui:AssignOperation.To>
                                  <ui:AssignOperation.Value>
                                    <InArgument x:TypeArguments="x:String" xml:space="preserve">Select ORDN5C As Order, OINU5C As InputDate, OCNU5C ConfirmDate, RDLU5C RequestDate, ALLOCATE Arr_date,
(Select MAX(PCPU9F) From WAVEDLIB.F9F00 Where RLTN9F=ORDN5C Group By RLTN9F) LASTCOMP,
(Select MAX(PAKU6P) From WAVEDLIB.S6P00 Where ORDN6P=ORDN5C Group By ORDN6P) LASTPACK,
MAX(NDCU5F) As EST_DELI, MAX(DLVU5F) As Delivery, SMPF5C As SampleFlag, Sum(AAFQ5F) As FINISHQTY,Sum(PAKQ5F) As PACKEDQTY, Sum(DCNQ5F) As DELIVERYQTY,
(Select SUM(ORRQ5E) From WAVEDLIB.S5E00 Where ORDN5E=ORDN5C Group By ORDN5E) SUM_QTY, NCMF5C 

From WAVEDLIB.S5C00    
INNER Join WAVEDLIB.S5F00 On ORDN5C = ORDN5F  
INNER Join (Select ORDN5B,MIN(OALU5B) ALLOCATE 
			From WAVEDLIB.S5B00 
			INNER Join WAVEDLIB.S5C00 On ORDN5C=ORDN5B 
			Group By ORDN5B,SMPF5C,PLTC5C 
			HAVING MIN(OALU5B)=( Select MAX(C2.YMDU02) 
								From WAVEDLIB.C0200 C1 
								INNER Join WAVEDLIB.C0200 C2 On C2.WDSV02 = C1.WDSV02  And C2.DPTC02=C1.DPTC02 And C2.HTYC02=0 
								Where C1.DPTC02='01' AND C1.YMDU02=?)  
								And SMPF5C='1' AND PLTC5C='2') A ON ORDN5C=ORDN5B 

Group By ORDN5C, OINU5C, OCNU5C, RDLU5C, SMPF5C, NCMF5C,ALLOCATE</InArgument>
                                  </ui:AssignOperation.Value>
                                </ui:AssignOperation>
